{
    "manifest_version": 3,
    "name": "WorkFlow",
    "description": "Work more efficiently",
    "version": "1.0",
    "icons": {
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "background":
    {
        "service_worker": "background.js",
        "persistant": true
    }
}